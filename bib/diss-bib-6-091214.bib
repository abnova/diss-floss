keywords = {DeLone and McLean IS Success Model,IS assessment,IS measurement,IS success,Information systems assessment,Information systems success,Measurement},
mendeley-tags = {DeLone and McLean IS Success Model,IS assessment,IS measurement,IS success},
number = {1},
