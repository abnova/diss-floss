journal = {European Management Review},
keywords = {FLOSS,FLOSS business models,commercial FLOSS development,competition,contribution,firms,involvement,open source software,sponsorship,users' skill level,variety},
mendeley-tags = {FLOSS,FLOSS business models,commercial FLOSS development,competition,contribution,firms,involvement,open source software,sponsorship,users' skill level,variety},
month = jan,
